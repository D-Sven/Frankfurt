<PropertyList>

   <path>eddf_tower.ac</path>

   <animation>
     <type>material</type>
     <object-name>Glass</object-name>
     <object-name>tower</object-name>
     <condition>
       <greater-than>
	 <property>/sim/time/sun-angle-rad</property>
	 <value>1.57</value>
       </greater-than>
     </condition>
     <emission>
       <red>1</red>
       <green>0.9</green>
       <blue>0.8</blue>
     </emission>
   </animation>

   <animation>
     <type>textranslate</type>
     <object-name>Glass</object-name>
     <object-name>tower</object-name>
     <property>/sim/time/sun-angle-rad</property>
     <step> 1.57 </step>
     <factor>0.318471338</factor>
     <center>
       <x-m> 0 </x-m>
       <y-m> 0 </y-m>
       <z-m> 0 </z-m>
     </center>
     <axis>
       <x> -1 </x>
       <y>  0 </y>
       <z>  0 </z>
     </axis>
   </animation>

   <animation>
     <type>rotate</type>
     <object-name>RADAR</object-name>
     <property>/sim/time/elapsed-sec</property>
     <factor>-200.0</factor>
     <center>
       <x-m>-14.000558</x-m>
       <y-m>0.000556</y-m>
       <z-m>77.178741</z-m>
     </center>
     <axis>
       <x>0</x>
       <y>0</y>
       <z>1</z>
     </axis>
   </animation>

   <animation>
     <type>select</type>
     <object-name>REDLIGHTDAY</object-name>
     <condition>
       <less-than-equals>
	 <property>/sim/time/sun-angle-rad</property>
	 <value>1.57</value>
       </less-than-equals>
     </condition>
   </animation>

   <model>
     <path>Models/Effects/RedLight10000.xml</path>
     <offsets>
       <x-m>-7.570531</x-m>
       <y-m>2.660119</y-m>
       <z-m>74.050003</z-m>
       <heading-deg>0</heading-deg>
     </offsets>
   </model>

   <model>
     <path>Models/Effects/RedLight10000.xml</path>
     <offsets>
       <x-m>-11.361687</x-m>
       <y-m>-6.383952</y-m>
       <z-m>74.050003</z-m>
       <heading-deg>0</heading-deg>
     </offsets>
   </model>

